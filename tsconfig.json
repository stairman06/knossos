{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "allowJs": true,
    "sourceMap": true,
    "noEmit": true,
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "~/*": [
        "./*"
      ],
      "@/*": [
        "./*"
      ],
    }
  },
  "exclude": [
    "node_modules",
    ".nuxt",
    "dist"
  ],
  "vueCompilerOptions": {
    "target": 2.7
  }
}
